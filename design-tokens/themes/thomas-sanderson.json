        "foreground": {
          "$type": "color",
          "$value": "{core.color.neutral.100}"
        }
      }
    },
    "content": {
      "primary": {
        "darker": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "darken",
                "value": "{other.color.modifiers.lightness-delta} * 4",
                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{core.color.omanticblue.30}",
          "$description": "Your primary and secondary brand colors are not necessarily your primary and secondary interaction colors, that's why I added those tokens."
        },
        "default": {
          "$type": "color",
          "$value": "{core.color.omanticblue.30}"
        },
        "lighter": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": "{other.color.modifiers.lightness-delta} * 4",
                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{core.color.omanticblue.30}"
        }
      },
      "secondary": {
        "darker": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "darken",
